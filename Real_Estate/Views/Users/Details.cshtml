@{
}
<h1>User Details</h1>

<div class="card" style="width: 18rem;">
    <div class="card-body">
        <h5 class="card-title">Name : @Model.Name</h5>
        <p class="card-text">Age : @Model.Age</p>
        <p class="card-text">Date of Birth : @Model.DOB</p>
        <p class="card-text">Address : @Model.Address</p>
        <p class="card-text">Phone Number : @Model.PhoneNumber</p>
        <p class="card-text">Url Image : @Model.UrlImages</p>
        <p class="card-text">User Name : @Model.UserName</p>
        <p class="card-text">Email : @Model.Email</p>
        <a asp-controller="Users" asp-action="GetAllUsers" class="card-link">Back to list</a>
    </div>
</div>